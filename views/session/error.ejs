<%- include("../modules/header.ejs", {
  pageTitle: pageTitle,
  pageDescription: "This feature has been disabled and is not accessible."
}) %>

<%- include("../modules/navigationBar.ejs", {
  features: features
}) %>

<%- include("../partials/miniHeader.ejs", {
  headerTitle: "Feature Disabled",
  backgroundImage: globalImage
}) %>

<section class="section">
  <div class="container">
    <div class="row">
      <div class="col-12 col-md-6" style="min-height:500px;">
        <div class="text-center">
          <img class="floating" style="margin-top:10px;" src="images/session/errorImage.png" height="300px" draggable="false">
        </div>
      </div>
      <div class="col-12 col-md-6">
          <h3>There was an error ;-;</h3>
          <% if (error.message) { %>
            <code><%= error.message %></code>
          <% } %>

          <% if (error) { %>
            <code><%= error %></code>
          <% } %>

          <br><br><br>
          <small class="text-muted">If this continues to happen, please report this by either <a style="color: rgb(75, 64, 224);" href="mailto:<%= config.siteConfiguration.email %>">e-mailing our Support Team</a> or <a style="color: rgb(75, 64, 224);" href="/discord">join our Community Discord and open a ticket</a>.</small>
      </div>
    </div>
  </div>
</section>

<%- include("../modules/footer.ejs") %>